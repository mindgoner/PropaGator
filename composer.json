{
    "name": "mindgoner/propagator",
    "description": "PropaGator - A Laravel package for propagation tracking",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mindgoner"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Mindgoner\\Propagator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mindgoner\\Propagator\\PropagatorServiceProvider"
            ],
            "aliases": {
                "Propagator": "Mindgoner\\Propagator\\Facades\\Propagator"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
